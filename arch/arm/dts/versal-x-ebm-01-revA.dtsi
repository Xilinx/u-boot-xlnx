status = "okay";
num-cs = <2>;
spi-tx-bus-width = <4>;
spi-rx-bus-width = <4>;
#address-cells = <1>;
#size-cells = <0>;
flash@0 {
	#address-cells = <1>;
	#size-cells = <1>;
	compatible = "m25p80", "jedec,spi-nor"; /* 256MB */
	reg = <0>, <1>;
	parallel-memories = /bits/ 64 <0x8000000 0x8000000>; /* 128MB */
	spi-tx-bus-width = <4>;
	spi-rx-bus-width = <4>;
	spi-max-frequency = <150000000>;
	partition@0 {
		label = "spi0-flash0";
		reg = <0x0 0x10000000>;
	};
};

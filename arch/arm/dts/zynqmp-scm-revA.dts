#include "zynqmp-sc-k24-revA.dts"

/ {
	model = "ZynqMP SCM RevA";
	compatible = "xlnx,zynqmp-scm-revA", "xlnx,zynqmp-scm",
		     "xlnx,zynqmp";

	memory@0 {
		device_type = "memory"; /* 4GB */
		reg = <0x0 0x0 0x0 0x80000000>, <0x8 0x00000000 0x0 0x80000000>;
	};
};

&zynqmp_dpsub {
	status = "disabled";
};

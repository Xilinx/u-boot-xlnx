#include <dt-bindings/gpio/gpio.h>

/dts-v1/;
/plugin/;

&{/} {
	compatible = "xlnx,zynqmp-scm-ve-p-a2112-00-revA",
		     "xlnx,zynqmp-scm-ve-p-a2112-00", "xlnx,zynqmp";
	model = "ZynqMP SCM VE-P-A2112-00 revA";

	sys-clk-0 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_hsdp>;
	};
	sys-clk-1 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_usb3>;
	};
	sys-clk-2 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_qsfp>;
	};
	sys-clk-3 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_lpddr5_c0>;
	};
	sys-clk-4 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_ddr5_udimm>;
	};
	sys-clk-5 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_xpl7_ps_refclk>;
	};
	sys-clk-6 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_lpddr5_c1>;
	};
	sys-clk-7 {
		status = "okay";
		compatible = "xlnx,fclk";
		clocks = <&xp_hdmi_frl_clk>;
	};
};

